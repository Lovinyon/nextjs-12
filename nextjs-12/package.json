{
  "name": "nextjs-latest-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "stylelint": "stylelint --ignore-path .gitignore './src/**/*.{css,scss,ts,tsx}'",
    "fix": "yarn lint --fix && yarn stylelint --fix && yarn format",
    "typecheck": "tsc",
    "prettier": "prettier --config .prettierrc.json './**/*.{ts,tsx,json,md}'",
    "format": "yarn prettier --write",
    "prepare": "husky install",
    "storybook": "storybook dev",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "autoprefixer": "^10.4.14",
    "next": "13.3.1",
    "postcss": "^8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/nextjs": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/testing-library": "^0.1.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "husky": "^8.0.3",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "storybook": "^7.0.6",
    "stylelint": "^15.6.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "5.0.4"
  }
}
